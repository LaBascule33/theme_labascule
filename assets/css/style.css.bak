/* Blocks  */
.block .text, 
.block .image {
    width: 100%;
    padding : 6% 4%;
}
    
@media (min-width: 768px) {
    .block .text,
    .block .image {
        width: 50%;
        padding : 6% 4%;
    } 
}

.block .image img {
    max-width: 100%;
    background-size: cover; 
}

.block .image {
    background-size: cover;
    background-position: center center;
    background-repeat: no-repeat;
}

@media (max-width: 768px) {
    .block .image {
        height: 500px; 
    }
}

.block .heading{
    position: absolute;
}

