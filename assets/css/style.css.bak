/* ---------------------------------------- */
/* --- Top Navbar ---*/
/* ---------------------------------------- */
#topNavbar{
    background-color: #fff;
    box-shadow: 0px 3px 6px #00000029;
}

.agenda {
    border-right: 1px solid #1F2650;
}

/* ---------------------------------------- */
/* ---  Navbar ---*/
/* ---------------------------------------- */
#header{
    position: fixed;
    /* top: 0; sans wpadminbar */
    top: 32px; /* avec wpadminbar */
    width: 100%;
    z-index: 999;
}

#pageContent{
    margin-top: 126px;
}

/* Menu */
#menu-home-menu > li a{
    font-family: "Avenir-Black";
    font-size: 0.875rem;
    text-transform: uppercase;
    color: #002B5E;
    border: 1px solid #fff;
}
#menu-home-menu > li
a:hover{
    background: #002B5E !important;
    color: #fff !important;
    border: 1px solid #fff !important;
}

.nav-link{
    margin: 0.1rem;
}

/* Logo */
.logo{
    max-width: 165px;
    width: 165px;
}

/* Background navbar*/
.bgw{
    background-color: rgba(255, 255, 255, 0.7);
    box-shadow: 0px 3px 6px #00000029;
}

/* ---------------------------------------- */
/* ---  Navbar Collapse ---*/
/* ---------------------------------------- */
.navbar {
    padding: 1rem;
}

@media (max-width: 991.98px) {
    #menu-home-menu > li a {
        font-family: "Avenir-Black";
        font-size: 1rem;
        text-transform: none;
        border: none !important;
    }

    #menu-home-menu > li
    a:hover{
    background: none !important;
    color: #002B5E !important;
    border: none !important;
    }

    #navbarMenu > #menu-home-menu {
        flex-direction: column;
    }

    #navbarMenu {
        padding-top: 10px;
        padding-bottom: 10px;
        padding-left: 0px;
        padding-right: 0px; 
    }
    
    .nav-link{
        padding-left: 0;
    }

    .btn.btn-primary{
        margin-top: 10px;
    }
}
@media (max-width: 767.98px){
    #topNavbar > .navbar > .container > .nav {
        width: 100%;
        padding-bottom: 10px !important;
        padding-top: 10px !important;
    }
}

.navbar-light .navbar-toggler {
    border: none;
    color: rgba(255, 255, 255, 0.5) !important;
    cursor: pointer;
}

.navbar-light.scrolled {
    position: fixed;
    right: 0;
    left: 0;
    top: 0;
    background: #fff !important;
    -webkit-box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.1);
    box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.1); 
}

