.btn{
    font-size: 0.8rem;
    font-family: "Avenir-Black";
    box-shadow: 0px 3px 6px #00000029;
    color: #ffff;
    border: 2px solid #FAFAFA;
    border-radius: 1rem;
    padding: .2rem 1rem;
    opacity: 1;
}

/* ---------------------------------------- */
/* --- Specials Btn --- */
/* ---------------------------------------- */

/* Btn-primary */
.btn.btn-primary{
    position: relative;
    background: #83B4FF;
}
.btn-primary:hover{
    background: #fff;
    color:#83B4FF;
    border: 2px solid #83B4FF;
}

.btn-primary.deco::before {
    position: absolute;
    top: -27px;
    right: -40px;
    width: 37px;
    height: 29px;
    background-image: url('../../img/apostrophes-gris.png');
    content: "";
}

/* Btn-soutien */
.btn.btn-soutien{
    position: relative;
    background: #83B4FF;
}
.btn-soutien:hover{
    background: #fff;
    color:#83B4FF;
    border: 2px solid #83B4FF;
}

/* Btn-SCT */
.btn.btn-SCT{
    background: #F786BE;
}

.btn-SCT:hover{
    background: #fff;
    color:#F786BE;
    border: 2px solid #F786BE;
}

.btn.r-100{
    border-radius: 50%;
    padding: 0.5rem  0.8rem;
}

/* Btn-cooperation */
.btn.btn-cooperation{
    background: #F9A485;
}

.btn-cooperation:hover{
    background: #fff;
    color:#F9A485;
    border: 2px solid #F9A485;
}

/* Btn-lobbying */
.btn.btn-lobbying{
    background: #C4E36A;
}

.btn-lobbying:hover{
    background: #fff;
    color:#C4E36A;
    border: 2px solid #C4E36A;
}

/* Dons */
.btn.btn-dons{
    background: #F786BE;
}

.btn-dons:hover{
    background: #fff;
    color:#F786BE;
    border: 2px solid #F786BE;
}

