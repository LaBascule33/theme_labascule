@font-face{
	font-family: "Avenir-Medium";
    src: url('./Fonts/Avenir-Medium.woff2');
}
@font-face{
	font-family: "Avenir-Black";
    src: url('./Fonts/Avenir-Black.woff2');
}

/* titles       :    h1          = Avenir Black  / (32px) 2rem
/* 2nd titles   :    h2,h3       = Avenir Black  / (24px) 1.5rem
/* subtitles    :    h4,h5,h6    = Avenir Medium / (24px) 1.5rem
/* paragraphes  :    p           = Avenir Book   / (16px) 1rem
/* buttons      :    btn         = Avenir Black  / (14px) 0.8rem

/* ---------------------------------------- */
/*--- Texts ---*/
/* ---------------------------------------- */
body {
    font-family: "Avenir-Medium", Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
    font-size: 1rem;
    color: #000;
}

span.bf,.bf-under{
    font-family: "Avenir-Black";
}

.p-blc{
    color: #fff;
}

/* ---------------------------------------- */
/* --- Titles --- */
/* ---------------------------------------- */


/* titles */
h1{
    font-family: "Avenir-Black";
    font-size: 2rem;
    color: #1F2650;
    position: relative;
}

/* 2nd titles */
/*h2, h3{
    font-family: "Avenir-Black";
    font-size: 1.5rem;
    color: #1F2650;
    position: relative;
}*/

h2 {
    font-family: "Avenir-Black";
    font-size: 2rem;
    color: #1F2650;
    position: relative;
    display: inline;
}

h2::before {
    position: absolute;
    z-index: -1;
    top: 70%;
    width: 105%;
    height: 50%;
    background-color: #d2dae4;
    border-radius: 1rem;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%) translateX(-2%);
    content: "";
}

h3{
    font-family: "Avenir-Black";
    font-size: 1.5rem;
    color: #1F2650;
    position: relative;
}


/* subtitles */
h4,h5,h6{
    font-family: "Avenir-Medium";
    font-size: 1.25rem;
    margin-bottom: 0;
}

/* paragraphes */
p{  
    font-family: "Avenir-Medium";
    font-size: 1rem;
    margin-bottom: 1.5rem;
}

/* line 
h1::before {
    width: 50%;
    height: 50%;
    background-color: #83B4FF;
    border-radius: 1rem;
    opacity: 1;
    position: absolute;
    z-index: -1;
    top: 70%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%) translateX(-2%);
    content: "";
} */


/* ---------------------------------------- */
/* --- Specials Titles --- */
/* ---------------------------------------- */
.text-center {
    text-align: center;
}

h3.text-center{
    letter-spacing: 6px;
    font-size: 1.25rem;
}

/* title-soutien */
.text-center.soutien{
    color: #83B4FF;
}

/* title-SCT */
.text-center.SCT{
    color: #F786BE;
}

/* title-cooperation */
.text-center.cooperation{
    color: #F9A485;
}

/* title-lobbying */
.text-center.lobbying{
    color: #C4E36A;
}


