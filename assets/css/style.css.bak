/* ---------------------------------------- */
/* --- FAQ ---*/
/* ---------------------------------------- */
#section-faq{
    background: url('../../img/faq/Bckgd-FAQ.svg');
    background-size: cover;
}

.block-img-faq {
    height: 80%;
    position: absolute;
    bottom: -4%;
}

/* Style Block */
.block-faq{
    padding: 6%;
    margin-left: auto;
    background-color: #002B5E;
    max-width: 100%;
}

a.faq{
    font-family: "Avenir-Medium";
    color: white !important;
    text-decoration: none;
    font-size: 1rem;
    letter-spacing: 0.9px;
}

a.faq span.FAQsize1{
    font-size: 1.5rem;
}

a.faq span.FAQsize2{
    font-size: 1.25rem;
}

/* Btn Block */
.btn-block{
    background: #002B5E;
    border: 4px solid #83B4FF;
    color: #fff;
    padding: 2rem;
    margin-block-end: 20px;
    opacity: 1;
    flex-wrap: wrap;
    cursor: pointer;
}

.btn-block:hover{
    background: #83B4FF;
    border: 4px solid #83B4FF;
    color: #fff;
}

/* ---------------------------------------- */
/* --- Modal FAQ --- */
/* ---------------------------------------- */
.modal-perso{
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.8);
    z-index: 99999;
    display: flex;
    align-items: center;
    justify-content: center;
}

/* Modal Style */
.modal-wrapper{
    overflow: hidden;
    background: #fff;
    width: 90%;
    height: 80%;
    /* max-width: calc(100vw - 150px); */
    /* max-height: calc(100vh - 100px); */
    padding: 4%;
    position: relative;
}

/* Btn Close */
button.closed{
    font-size: 4rem;
    color: #00204A;
    position: absolute;
    top: 0;
    right: 5%;
    align-self: end;
    z-index: 1;
    border: none;
    background: none;
}

/* Btn Slide */
button.prev-modal{
    position: absolute;
    height: 70px;
    bottom: 2.5%;
    left: 5%;
    z-index: 1;
    border: none;
    background: transparent;
}

button.prev-modal   :focus 
                    :active,
button.next-modal   :focus
                    :active{
    border: none;
    outline: none;
}

button.next-modal{
    position: absolute;
    height: 70px;
    bottom: 2.5%;
    right: 5%;
    z-index: 1;
    border: none;
    background: transparent;
}


span.text-prev-modal,
span.text-next-modal{
    border: 4px solid #fff;
    padding: 10px;
}

.text-next-modal:hover, 
.text-prev-modal:hover{
    border: 4px solid #83B4FF;
}

.slide{
    position: absolute;
    top: 52px;
    left: 50%;
    width: 1154px;
    height: 463px;
    background-color: #E7F0FC;
    opacity: 0;
    transform: translateX(-50%);
}

.slide-active{
    animation: effet-modal 3s step-end infinite;
}

@keyframes effet-modal {
    from,
    to {
        opacity: 1;
    }
}


